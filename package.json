  },
  "homepage": "https://github.com/mfranzke/loading-attribute-polyfill",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@wdio/cli": "^6.0.13",
